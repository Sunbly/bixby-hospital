capsule {
  id (playground.hospital)
  version (0.1.0)
  format (3)
  targets {
    target (bixby-mobile-ko-KR)
  }
  runtime-flags {
    concepts-inherit-super-type-features
    modern-prompt-rejection
    support-halt-effect-in-computed-inputs
  }

  capsule-imports {
    import (viv.geo) {
      as (geo)
      version (9.20.3)
    }
    import (viv.self){
      as(self)
      version (3.5.154)
    }
  }
  permissions {
    user-profile-access
  }
}
